= form_for :drawing, :url => to_path, data: {class: 'file_upload_form'}, html: { multipart: true, class: 'file_upload_form' }  do |form|
  .photoPreview
    p#uploadClick Upload your drawing

  = form.file_field :avatar, accept: 'image/png,image/gif,image/jpeg,image/jpg', id: 'uploadCard', data: { id: 'uploadCard' }, multiple: allow_multiple_file_upload
  = submit_tag 'Submit Photo', id: 'submitPhoto', class: 'btn btn-primary', disabled: true

